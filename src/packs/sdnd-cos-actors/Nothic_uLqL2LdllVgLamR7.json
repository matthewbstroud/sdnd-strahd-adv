    {
      "_id": "CerisJBqjzGtGaVY",
      "name": "Petrifying Gaze",
      "type": "feat",
      "img": "icons/magic/control/hypnosis-mesmerism-eye.webp",
      "effects": [],
      "folder": null,
      "sort": 0,
      "flags": {
        "core": {
          "sourceId": "Compendium.dnd5e.monsterfeatures.wkIN7WTeX8ebbjtv"
